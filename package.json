{
  "name": "asocial-bookmark",
  "version": "2.0.1",
  "description": "personal bookmark system.",
  "keywords": [
    "bookmark"
  ],
  "homepage": "https://github.com/azu/asocial-bookmark",
  "bugs": {
    "url": "https://github.com/azu/asocial-bookmark/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/asocial-bookmark.git"
  },
  "license": "MIT",
  "author": "azu",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "asocial-bookmark-create-index": "./lib/cli/asocial-bookmark-create-index-cli.js",
    "migrate-hatenabookmark-to-asocial-bookmark": "./lib/cli/migrate-hatenabookmark-to-asocial-bookmark-cli.js"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublishOnly": "npm run --if-present build",
    "test": "vitest run",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "dayjs": "^1.8.14",
    "debug": "^4.1.1",
    "fast-deep-equal": "^3.0.0",
    "fromfrom": "^1.3.0",
    "glob": "^11.0.0",
    "hatebu-mydata-parser": "^1.0.0",
    "korefile": "^2.1.0",
    "meow": "^13.0.0",
    "normalize-url": "^8.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
